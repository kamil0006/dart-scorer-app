{
	"expo": {
		"name": "dart-scorer",
		"slug": "dart-scorer",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "dartscorer",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#121212"
			},
			"edgeToEdgeEnabled": true,
			"package": "com.anonymous.dartscorer"
		},
		"plugins": [
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#121212"
				}
			],
			"expo-sqlite"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
